<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
  targetNamespace="atr" xmlns="atr" xmlns:objtyp="objtyp" xmlns:gml="http://www.opengis.net/gml/3.2">
  <xs:import schemaLocation="../ext/gml/gml.xsd" namespace="http://www.opengis.net/gml/3.2"/>

  <xs:element name="SpolecneAtributyVsechObjektu">
    <xs:complexType>
      <xs:all>
        <xs:element ref="ID" minOccurs="0"/>
        <xs:element ref="IDZmeny" minOccurs="0"/>
        <xs:element ref="PopisObjektu" minOccurs="0"/>
        <xs:element ref="IDEditora" minOccurs="0"/>
        <xs:element ref="DatumVkladu" minOccurs="0"/>
        <xs:element ref="VkladOsoba" minOccurs="0"/>
        <xs:element ref="DatumZmeny" minOccurs="0"/>
        <xs:element ref="ZmenaOsoba" minOccurs="0"/>
<!-- zrušeno v JVF 1.4.3
        <xs:element ref="DatumPlatnosti"/>
-->
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="ID" type="xs:string">
    <xs:annotation>
      <xs:documentation>Unikátní identifikátor objektu v rámci DTM ČR, zadává IS DTMK</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IDZmeny" type="xs:string">
    <xs:annotation>
      <xs:documentation>Jednoznačný identifikátor změny, generuje IS DMVS jako ID Podání</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IDZmenyZpracovatele" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor změny v IS zpracovatele, zadává zpracovatel</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PopisObjektu" type="xs:string">
    <xs:annotation>
      <xs:documentation>Popis objektu, zadává zpracovatel</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IDEditora" type="xs:string">
    <xs:annotation>
      <xs:documentation>Jednoznačný identifikátor editora (kraje nebo správce vymezeného území)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DatumVkladu" type="xs:dateTime">
    <xs:annotation>
      <xs:documentation>Datum a čas zapracování objektu do DTMK, zadává IS DTMK</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="VkladOsoba" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor fyzické osoby, která provedla vklad objektu, zadává IS DTMK</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DatumZmeny" type="xs:dateTime">
    <xs:annotation>
      <xs:documentation>Datum a čas prvotního vložení nebo poslední změny objektu v DTMK, zadává IS DTMK</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ZmenaOsoba" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor fyzické osoby, která provedla poslední změnu objektu, zadává IS DTMK</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ICS" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikační číslo stavby. Případných více záznamů IČS odděleno středníkem.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuZPS">
    <xs:annotation>
      <xs:documentation>Společné atributy objektů základní prostorové situace s výjimkou definičních bodů a podrobných bodů</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="UrovenUmisteniObjektuZPS"/>
        <xs:element ref="TridaPresnostiPoloha"/>
        <xs:element ref="TridaPresnostiVyska"/>
        <xs:element ref="ZpusobPorizeniZPS"/>
        <xs:element ref="ICS" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuDefinicnichBodu">
    <xs:annotation>
      <xs:documentation>Společné atributy objektů definičních bodů</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="UrovenUmisteniObjektuZPS"/>
        <xs:element ref="ICS" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="UrovenUmisteniObjektuZPS">
    <xs:annotation>
      <xs:documentation>Úroveň umístění objektu ZPS vzhledem k povrchu a jiným objektům DTI a ZPS</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="-3">
          <xs:annotation>
            <xs:documentation>3. úroveň pod povrchem (nejníže)</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="-2">
          <xs:annotation>
            <xs:documentation>2. úroveň pod povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="-1">
          <xs:annotation>
            <xs:documentation>1. úroveň pod povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="0">
          <xs:annotation>
            <xs:documentation>na povrchu</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>1. úroveň nad povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="2">
          <xs:annotation>
            <xs:documentation>2. úroveň nad povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="3">
          <xs:annotation>
            <xs:documentation>3. úroveň nad povrchem (nejvýše)</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuTI">
    <xs:annotation>
      <xs:documentation>Společné atributy objektů technické infrastruktury s výjimkou objektů záměrů a pásem TI</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="IDVlastnika" minOccurs="0"/>
        <xs:element ref="IDProvozovateleZeZakona" minOccurs="0"/>
        <xs:element ref="IDSpravce" minOccurs="0"/>
        <xs:element ref="IDProvozovatele" minOccurs="0"/>
        <xs:element ref="IDExterni" minOccurs="0"/>
        <xs:element ref="NeuplnaData"/>
        <xs:element ref="UrovenUmisteniObjektuTI"/>
        <xs:element ref="TridaPresnostiPoloha"/>
        <xs:element ref="TridaPresnostiVyska"/>
        <xs:element ref="ZpusobPorizeniTI"/>
        <xs:element ref="EvidencniCisloObjektu" minOccurs="0"/>
        <xs:element ref="ICS" minOccurs="0"/>
        <xs:element ref="KritickaTI" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuPasemTI">
    <xs:annotation>
      <xs:documentation>Společné atributy objektů ochranných a bezpečnostních pásem technické infrastruktury</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="IDVlastnika" minOccurs="0"/>
        <xs:element ref="IDProvozovateleZeZakona" minOccurs="0"/>
        <xs:element ref="IDSpravce" minOccurs="0"/>
        <xs:element ref="IDProvozovatele" minOccurs="0"/>
        <xs:element ref="IDExterni" minOccurs="0"/>
        <xs:element ref="NeuplnaData"/>
        <xs:element ref="UrovenUmisteniObjektuTI"/>
        <xs:element ref="TridaPresnostiPoloha"/>
        <xs:element ref="ZpusobPorizeniTI"/>
        <xs:element ref="EvidencniCisloObjektu" minOccurs="0"/>
        <xs:element ref="KritickaTI" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuZPS_TI">
    <xs:annotation>
      <xs:documentation>Společné atributy objektů ZPS typu TI (přípojky odběrného místa)</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="UrovenUmisteniObjektuTI"/>
        <xs:element ref="TridaPresnostiPoloha"/>
        <xs:element ref="TridaPresnostiVyska"/>
        <xs:element ref="ZpusobPorizeniTI"/>
        <xs:element ref="StavObjektu"/>
        <xs:element ref="ICS" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuDI">
    <xs:annotation>
      <xs:documentation>Společné atributy objektů dopravní infrastruktury s výjimkou objektů záměrů a pásem DI</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="IDVlastnika" minOccurs="0"/>
        <xs:element ref="IDSpravce" minOccurs="0"/>
        <xs:element ref="IDProvozovatele" minOccurs="0"/>
        <xs:element ref="IDExterni" minOccurs="0"/>
        <xs:element ref="NeuplnaData"/>
        <xs:element ref="UrovenUmisteniObjektuDI"/>
        <xs:element ref="TridaPresnostiPoloha"/>
        <xs:element ref="TridaPresnostiVyska"/>
        <xs:element ref="ZpusobPorizeniDI"/>
        <xs:element ref="EvidencniCisloObjektu" minOccurs="0"/>
        <xs:element ref="ICS" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuPasemDI">
    <xs:annotation>
      <xs:documentation>Společné atributy objektů ochranných a bezpečnostních pásem dopravní infrastruktury</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="IDVlastnika" minOccurs="0"/>
        <xs:element ref="IDSpravce" minOccurs="0"/>
        <xs:element ref="IDProvozovatele" minOccurs="0"/>
        <xs:element ref="IDExterni" minOccurs="0"/>
        <xs:element ref="NeuplnaData"/>
        <xs:element ref="UrovenUmisteniObjektuDI"/>
        <xs:element ref="TridaPresnostiPoloha"/>
        <xs:element ref="ZpusobPorizeniDI"/>
        <xs:element ref="EvidencniCisloObjektu" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="SpolecneAtributyObjektuZameru">
    <xs:complexType>
      <xs:all>
        <xs:element ref="IDVlastnika" minOccurs="0"/>
        <xs:element ref="IDSpravce" minOccurs="0"/>
        <xs:element ref="IDProvozovatele" minOccurs="0"/>
        <xs:element ref="IDExterni" minOccurs="0"/>
        <xs:element ref="EvidencniCisloObjektu" minOccurs="0"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="OblastObjektuKI">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="gml:surfaceProperty"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="IDVlastnika" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor vlastníka - vazba do seznamu vlastníků, správců a provozovatelů</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IDProvozovateleZeZakona" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor provozovatele ze zákona - vazba do seznamu vlastníků, správců a provozovatelů</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IDSpravce" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor správce - vazba do seznamu vlastníků, správců a provozovatelů</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IDProvozovatele" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor provozovatele - vazba do seznamu vlastníků, správců a provozovatelů</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IDExterni" type="xs:string">
    <xs:annotation>
      <xs:documentation>Identifikátor objektu v externím systému editora</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="NeuplnaData" type="xs:boolean">
    <xs:annotation>
      <xs:documentation>Jedná se o neúplná data o objektu podle $ 4 Vyhlášky o DTM?</xs:documentation>
      <xs:documentation>0 = ne, data o objektu jsou úplná</xs:documentation>
      <xs:documentation>1 = ano, data o objektu nejsou úplná</xs:documentation>
    </xs:annotation>
    <!--
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="0">
          <xs:annotation>
            <xs:documentation>ne, data o objektu jsou úplná</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>ano, data o objektu nejsou úplná</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
-->
  </xs:element>
  <xs:element name="UrovenUmisteniObjektuTI">
    <xs:annotation>
      <xs:documentation>Úroveň umístění objektu TI vzhledem k povrchu</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="-1">
          <xs:annotation>
            <xs:documentation>pod povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="0">
          <xs:annotation>
            <xs:documentation>na povrchu</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>nad povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="UrovenUmisteniObjektuDI">
    <xs:annotation>
      <xs:documentation>Úroveň umístění objektu DI vzhledem k povrchu a jiným objektům DTI a ZPS</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="-3">
          <xs:annotation>
            <xs:documentation>3. úroveň pod povrchem (nejníže)</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="-2">
          <xs:annotation>
            <xs:documentation>2. úroveň pod povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="-1">
          <xs:annotation>
            <xs:documentation>1. úroveň pod povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="0">
          <xs:annotation>
            <xs:documentation>na povrchu</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>1. úroveň nad povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="2">
          <xs:annotation>
            <xs:documentation>2. úroveň nad povrchem</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="3">
          <xs:annotation>
            <xs:documentation>3. úroveň nad povrchem (nejvýše)</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="EvidencniCisloObjektu" type="xs:string">
    <xs:annotation>
      <xs:documentation>Evidenční číslo objektu</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="KritickaTI" type="xs:boolean">
    <xs:annotation>
      <xs:documentation>Jedná se o kritickou technickou infrastrukturu?</xs:documentation>
      <xs:documentation>0 = ne, 1 = ano</xs:documentation>
      <xs:documentation>pokud není element uveden, nejedná se o KI</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="IDPodani" type="xs:string">
    <xs:annotation>
      <xs:documentation>Jednoznačný identifikátor podání</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="OznaceniKomunikace" type="xs:string">
    <xs:annotation>
      <xs:documentation>Označení pozemní komunikace</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PrevazujiciPovrch">
    <xs:annotation>
      <xs:documentation>Převažující povrch</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>asfalt</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="2">
          <xs:annotation>
            <xs:documentation>beton</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="3">
          <xs:annotation>
            <xs:documentation>dlažba</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="4">
          <xs:annotation>
            <xs:documentation>R-materiál</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="5">
          <xs:annotation>
            <xs:documentation>písek, štěrkopísek</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="6">
          <xs:annotation>
            <xs:documentation>šotolina</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="7">
          <xs:annotation>
            <xs:documentation>nezpevněno</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="99">
          <xs:annotation>
            <xs:documentation>nezjištěno</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="KategoriePozemniKomunikace">
    <xs:annotation>
      <xs:documentation>Kategorie pozemní komunikace</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>dálnice I. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="2">
          <xs:annotation>
            <xs:documentation>dálnice II. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="3">
          <xs:annotation>
            <xs:documentation>silnice I. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="4">
          <xs:annotation>
            <xs:documentation>silnice II. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="5">
          <xs:annotation>
            <xs:documentation>silnice III. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="6">
          <xs:annotation>
            <xs:documentation>místní komunikace I. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="7">
          <xs:annotation>
            <xs:documentation>místní komunikace II. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="8">
          <xs:annotation>
            <xs:documentation>místní komunikace III. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="9">
          <xs:annotation>
            <xs:documentation>místní komunikace IV. třídy</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="10">
          <xs:annotation>
            <xs:documentation>účelová komunikace</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="11">
          <xs:annotation>
            <xs:documentation>neveřejně přístupná účelová komunikace</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="99">
          <xs:annotation>
            <xs:documentation>nezjištěno/neurčeno</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="CisloETahu" type="xs:string">
    <xs:annotation>
      <xs:documentation>Číslo E-Tahu</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="TypUsekuPozemniKomunikace">
    <xs:annotation>
      <xs:documentation>Typ úseku pozemní komunikace</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>zemní těleso komunikace</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="2">
          <xs:annotation>
            <xs:documentation>most</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="3">
          <xs:annotation>
            <xs:documentation>tunel</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="4">
          <xs:annotation>
            <xs:documentation>přesypaný most</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="99">
          <xs:annotation>
            <xs:documentation>nezjištěno</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="PocetJizdnichPruhu" type="xs:nonNegativeInteger">
    <xs:annotation>
      <xs:documentation>Počet jízdních pruhů</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="TypPozemniKomunikace">
    <xs:annotation>
      <xs:documentation>Typ pozemní komunikace</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>komunikace</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="2">
          <xs:annotation>
