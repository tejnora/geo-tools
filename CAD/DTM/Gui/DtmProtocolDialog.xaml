<gui:DialogBase x:Class="CAD.DTM.Gui.DtmProtocolDialog"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                xmlns:gui="clr-namespace:GeoBase.Gui;assembly=GeoBase"
                xmlns:localization="clr-namespace:GeoBase.Localization;assembly=GeoBase"
                mc:Ignorable="d" 
                d:DesignHeight="300" d:DesignWidth="300">
    <gui:DialogBase.DialogContent>
        <TextBox  FontFamily="Courier New" FontSize="20" TextWrapping="NoWrap" Foreground="Black"  Text="{Binding Path=Text}"/>
    </gui:DialogBase.DialogContent>
    <gui:DialogBase.CustomButtons>
        <Button localization:Translate.Uid="161" Content="{localization:Translate Cancel}" Click="OnClose" />
    </gui:DialogBase.CustomButtons>
</gui:DialogBase>
